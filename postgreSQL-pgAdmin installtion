1 sudo -u postgres psql
2 sudo apt-get update
3 sudo curl https://www.pgadmin.org/static/packages_pgadmin_org.pub | sudo apt-key add
4 sudo sh -c 'echo "deb https://ftp.postgresql.org/pub/pgadmin/pgadmin4/apt/$(lsb_release -cs) pgadmin4 main" > /etc/apt/sources.list.d/pgadmin4.list && apt update

5 sudo sh -c 'echo "deb https://ftp.postgresql.org/pub/pgadmin/pgadmin4/apt/$(lsb_release -cs) pgadmin4 main" > /etc/apt/sources.list.d/pgadmin4.list && apt update'

6 sudo apt istall pgadmin4









